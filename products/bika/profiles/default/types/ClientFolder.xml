<?xml version="1.0"?>
<object name="ClientFolder"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">ClientFolder</property>
 <property name="description"></property>
 <property name="icon_expr">string:${portal_url}/folder_icon.gif</property>
 <property name="content_meta_type">ClientFolder</property>
 <property name="product">bika</property>
 <property name="factory">addClientFolder</property>
 <property name="add_view_expr"></property>
 <property name="link_target"></property>
 <property name="immediate_view">clientfolder_contents</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Client"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">clientfolder_contents</property>
 <property name="view_methods">
  <element value="clientfolder_contents"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="base_edit"/>
 <alias from="index.html" to="(dynamic view)"/>
 <alias from="properties" to="base_metadata"/>
 <alias from="sharing" to="folder_localrole_form"/>
 <alias from="view" to="(selected layout)"/>
 <action title="Copy" action_id="copy" category="folder_buttons"
    condition_expr="" icon_expr="" link_target="" url_expr=""
    visible="False"/>
 <action title="Cut" action_id="cut" category="folder_buttons"
    condition_expr="" icon_expr="" link_target="" url_expr=""
    visible="False"/>
 <action title="Paste" action_id="paste" category="folder_buttons"
    condition_expr="" icon_expr="" link_target="" url_expr=""
    visible="False"/>
 <action title="Rename" action_id="rename" category="folder_buttons"
    condition_expr="" icon_expr="" link_target="" url_expr=""
    visible="False"/>
 <action title="Change State" action_id="change_state"
    category="folder_buttons" condition_expr="" icon_expr="" link_target=""
    url_expr="" visible="False"/>
 <action title="Properties" action_id="metadata" category="object"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/base_metadata" visible="False"/>
 <action title="Sharing" action_id="local_roles" category="object"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/@@sharing" visible="False"/>
 <action title="Contents" action_id="folderContents" category="object"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${globals_view/getCurrentFolderUrl}/folder_contents"
    visible="False"/>
</object>
