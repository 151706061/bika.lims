<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="bika">

    <browser:page
      for="bika.lims.interfaces.IBatch"
      name="getAetiologicAgents"
      class="bika.lims.browser.widget.caseaetiologicagentswidget.ajaxGetAetiologicAgents"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMS"
    />
    
    <browser:page
      for="bika.lims.interfaces.IBatch"
      name="getAetiologicAgentSubtypes"
      class="bika.lims.browser.widgets.caseaetiologicagentswidget.ajaxGetAetiologicAgentSubtypes"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMS"
    />
    
</configure>