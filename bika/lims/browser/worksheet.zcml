<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="bika">

    <browser:page
      for="bika.lims.interfaces.IWorksheetFolder"
      name="worksheets"
      class="bika.lims.browser.worksheet.WorksheetFolderView"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMSLayer"
    />

    <browser:page
      for="bika.lims.interfaces.IWorksheetFolder"
      name="worksheet_add"
      class="bika.lims.browser.worksheet.WorksheetAddView"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMSLayer"
    />

    <browser:page
      for="bika.lims.interfaces.IWorksheet"
      name="worksheet_analyses"
      class="bika.lims.browser.worksheet.WorksheetAnalysesView"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMSLayer"
    />

    <browser:resource
      name="worksheet.js"
      file="worksheet.js"
      layer="bika.lims.interfaces.IBikaLIMSLayer"
    />

</configure>
