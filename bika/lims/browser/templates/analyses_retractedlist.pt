<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="bika">

<head>
    <link rel="stylesheet" type="text/css" media="all" href=""
          tal:attributes="href string:$portal_url/analysisrequest_results.css" />
</head>

<body tal:define="analyses python:view.analyes">
    <h1 i18n:translate="">Retracted analyses</h1>
    <ul>
        <tal:analyses tal:repeat="rows python:view.getData()">
            <li>
                <span tal:content="python: row['an_id']"></span>,&nbsp;
                <span tal:content="python: row['an_title']"></span>
                <tal:ar condition="python: row['ar']">
                    ,&nbsp;<span i18n:translate="">Analysis Request</span>:&nbsp;
                    <span tal:content="python: row['ar_html']"></span>
                </tal:ar>
                <tal:ar condition="python: row['ws']">
                    ,&nbsp;<span i18n:translate="">Worksheet</span>:&nbsp;
                    <span tal:content="python: row['ws_html']"></span>
                </tal:ar>
            </li>
        </tal:analyses>
    </ul>
</body>
</html>
