<?xml version="1.0"?>
<object name="SamplePoints"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">Sample Points</property>
 <property name="description">
   The places where samples are routinely taken, Including geographical co-ordinates.
   Could be a certain bore hole for water samples, or point on production line in a factory etc.
 </property>
 <property name="content_icon">++resource++bika.lims.images/samplepoint.png</property>
 <property name="content_meta_type">SamplePoints</property>
 <property name="product">bika.lims</property>
 <property name="factory">addSamplePoints</property>
 <property name="add_view_expr"></property>
 <property name="link_target"></property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="SamplePoint"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="folder_view"/>
 <alias from="view" to="folder_view"/>
 <alias from="edit" to="base_edit"/>
</object>
